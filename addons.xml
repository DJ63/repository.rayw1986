<addons>
<addon id="plugin.video.freeview" name="Freeview" version="3.1.2" provider-name="rayw1986">
    <requires>
        <import addon="xbmc.python" version="2.1.0"/>
    </requires>
    <extension point="xbmc.python.pluginsource"
               	library="default.py">
        <provides>video</provides>
    </extension>
    <extension point="xbmc.addon.metadata">
        <platform>all</platform>
        <summary>UK Freeview Channels</summary>
        <description>Watch a wide array of Freeview channels from official and legal sources.</description>
		<disclaimer lang="en">The author is not responsible for the media you find in this add-on.</disclaimer>
    </extension>

</addon>

