<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="plugin.video.iwn"
       name="Indy Wrestling Network"
       version="1.0.0"
       provider-name="RayW1986">
    <requires>
        <import addon="xbmc.python" version="2.1.0"/>
    </requires>
    <extension point="xbmc.python.pluginsource"
               	library="default.py">
        <provides>video</provides>
    </extension>
    <extension point="xbmc.addon.metadata">
        <platform>all</platform>
        <summary>Collection of Independent Wrestling Promotions</summary>
        <description>The Indy Wrestling Network is your number one stop shop for independent wrestling promotions across the world.
		[B]NOTE:[/B] This plugin requires that you have the YouTube plugin installed also.</description>
    </extension>

</addon>

<addon id="repository.rayw1986" name="RayW1986 Repo" version="1.0" provider-name="rayw1986">
	<extension point="xbmc.addon.repository" name="RayW1986 Repository">
		<info compressed="false">http://raw.github.com/RayW1986/repository.rayw1986/master/addons.xml</info>
		<checksum>http://raw.github.com/RayW1986/repository.rayw1986/master/addons.xml.md5</checksum>
		<datadir zip="true">http://raw.github.com/RayW1986/repository.rayw1986/master/zips</datadir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary>RayW1986's Repository</summary>
		<description>RayW1986's Repository. Includes the Indy Wrestling Network.</description>
		<platform>all</platform>
	</extension>
</addon>
</addons>
